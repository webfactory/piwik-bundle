{
    "name": "webfactory/piwik-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that adds a twig-function for the Matomo tracking code.",
    "license": "MIT",
    "authors": [
        {
            "name": "webfactory GmbH",
            "email": "info@webfactory.de",
            "homepage": "https://www.webfactory.de",
            "role": "Developer"
        },
        {
            "name": "Tim Bernhard",
            "email": "tim@genieblog.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webfactory\\Bundle\\PiwikBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webfactory\\Bundle\\PiwikBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">= 8.1",
        "ext-json": "*",
        "symfony/config": "^6.4 || ^7.3 || ^8.0",
        "symfony/dependency-injection": "^6.4 || ^7.3 || ^8.0",
        "symfony/http-kernel": "^6.4 || ^7.3 || ^8.0",
        "twig/twig": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.58"
    },
    "config": {
        "sort-packages": true
    }
}
